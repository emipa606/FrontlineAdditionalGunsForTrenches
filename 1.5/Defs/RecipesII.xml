      <thingDefs>
        <li>Chemfuel</li>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <TR_AmmoLMGII>1</TR_AmmoLMGII>
    </products>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
  </RecipeDef>
